name: dockercompose6909268475706169932
services:
  musifyapi:
    build:
      context: /Users/kamsi/Documents/School2/5th semester/COMP 306 API Engineering
        & cloud/group/musify-app
      dockerfile: MusifyAPI/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: MusifyAPI
      target: base
    container_name: MusifyAPI
    entrypoint:
    - tail
    - -f
    - /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      NUGET_FALLBACK_PACKAGES: ""
    image: musifyapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net7.0/MusifyAPI.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    - mode: ingress
      target: 443
      protocol: tcp
    tty: true
    volumes:
    - type: bind
      source: /Users/kamsi/Documents/School2/5th semester/COMP 306 API Engineering
        & cloud/group/musify-app/MusifyAPI
      target: /app
      bind:
        create_host_path: true
    - type: bind
      source: /Users/kamsi/vsdbg/vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: /Users/kamsi/.aspnet/https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: /Users/kamsi/.microsoft/usersecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: /Users/kamsi/.nuget/packages/
      target: /root/.nuget/packages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: /Users/kamsi/Documents/School2/5th semester/COMP 306 API Engineering
        & cloud/group/musify-app
      target: /src
      bind:
        create_host_path: true
networks:
  default:
    name: dockercompose6909268475706169932_default